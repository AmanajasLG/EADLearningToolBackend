{
  "kind": "collectionType",
  "collectionName": "play_sessions",
  "info": {
    "name": "playSession",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "score": {
      "type": "integer"
    },
    "sessionTime": {
      "type": "float"
    },
    "finished": {
      "type": "boolean",
      "default": false
    },
    "win": {
      "type": "boolean",
      "default": false
    },
    "usersPermissionsUser": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "playSessions"
    },
    "game": {
      "model": "game",
      "via": "playSessions"
    },
    "mission": {
      "via": "playSessions",
      "model": "mission"
    },
    "userGameAnswer": {
      "model": "user-game-answer",
      "via": "playSession"
    },
    "userQuestionnaireAnswer": {
      "model": "user-questionnaire-answer",
      "via": "playSession"
    },
    "playerActions": {
      "type": "component",
      "repeatable": true,
      "component": "json-data.player-action"
    }
  }
}
